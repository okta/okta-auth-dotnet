{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/Okta.Auth.Sdk.csproj", "**/Okta.Sdk.Abstractions.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**", "docs/**" ],
          "src": "../"
		  }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Okta .NET Authentication SDK Documentation",
      "_appFooter": "Copyright &copy; 2020 Okta, Inc.<br>Generated with <b>DocFx</b>.",
      "_appLogoPath": "images/okta-dev-logo.png",
      "_appFaviconPath": "images/favicon.ico",
      "_disableContribution": "true" 
	},
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}